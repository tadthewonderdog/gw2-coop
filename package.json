{
  "name": "gw2-coop",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "build:preview": "tsc -b && vite build --mode preview",
    "lint": "NODE_OPTIONS='--max-old-space-size=8192' eslint .",
    "lint:fix": "NODE_OPTIONS='--max-old-space-size=8192' eslint . --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,css,md,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,css,md,json}\"",
    "type-check": "tsc --noEmit",
    "preview": "vite preview",
    "test": "vitest run",
    "test:storybook": "vitest run --project=storybook",
    "test:unit": "vitest run --project=unit",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "validate": "npm run lint && npm run format:check && npm run type-check",
    "validate:prepush": "npm run test:full && npm run validate",
    "analyze:css": "npx tailwindcss -i ./src/index.css -o ./dist/output.css --minify && npx gzip-size ./dist/output.css",
    "analyze:bundle": "npm run build && npx vite-bundle-analyzer dist",
    "fetch:gw2-achievement-data": "node --loader ts-node/esm --require tsconfig-paths/register src/scripts/fetch-gw2-achievement-data.ts",
    "security:audit": "npm audit --audit-level=high",
    "security:fix": "npm audit fix",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "deploy": "wrangler deploy"
  },
  "dependencies": {
    "@radix-ui/react-accordion": "^1.2.11",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.5",
    "@shadcn/ui": "^0.0.4",
    "@tanstack/react-query": "^5.83.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "hono": "^4.8.5",
    "lucide-react": "^0.539.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.6.1",
    "tailwind-merge": "^3.3.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^4.0.5",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.0.1",
    "@cloudflare/vite-plugin": "^1.9.6",
    "@cloudflare/workers-types": "^4.20250719.0",
    "@storybook/addon-a11y": "^9.0.16",
    "@storybook/addon-docs": "^9.0.16",
    "@storybook/addon-vitest": "^9.0.16",
    "@storybook/react-vite": "^9.0.16",
    "@tailwindcss/postcss": "^4.1.11",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.0.13",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@typescript-eslint/eslint-plugin": "^8.36.0",
    "@typescript-eslint/parser": "^8.36.0",
    "@vitejs/plugin-react": "^5.0.0",
    "@vitest/browser": "3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "axe-core": "^4.10.3",
    "eslint": "^9.31.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^4.4.3",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "eslint-plugin-sonarjs": "^3.0.2",
    "eslint-plugin-storybook": "^9.0.16",
    "jsdom": "^26.1.0",
    "markdown-to-jsx": "^7.7.10",
    "node-fetch": "^3.3.2",
    "playwright": "^1.54.1",
    "postcss": "^8.5.6",
    "prettier": "^3.2.5",
    "storybook": "^9.0.16",
    "terser": "^5.43.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "~5.8.3",
    "vite": "^7.1.1",
    "vite-bundle-analyzer": "^1.1.0",
    "vitest": "^3.2.4",
    "wrangler": "^4.28.0"
  }
}
